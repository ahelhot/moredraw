{
    "name": "ahelhot/moredraw",
    "description": "Handlebars two-side wrapper (js/php) - Same template anywhere with reactive update",
    "repositories": [
      {
        "type": "git",
        "url": "https://github.com/ahelhot/moredraw"
      }
    ],
    "keywords": ["handlebars", "PHP", "template", "logicless", "JS"],
    "type": "template",
    "version": "0.6.6",
    "require": {
        "php": ">=7",
        "zordius/lightncandy": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "NeoHandlebars\\": "src"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "HC",
            "email": "fe3dback@yandex.ru"
        }
    ],
    "minimum-stability": "stable",
    "require-dev": {
        "satooshi/php-coveralls": "^1.0",
        "phpunit/phpcov": "^3.1"
    }
}
